package selfProgramming;

class Float {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.printf("%s","hello");
		System.out.println("");
		System.out.printf("%S","hello");
		System.out.println("");
		System.out.printf("%c",'a');
		System.out.println("");
		System.out.printf("%C",'a');
		System.out.println("");
		System.out.printf("%b", 5 < 4);
		System.out.println("");
		System.out.printf("%B", 5 > 4);
		System.out.println("");

	}

}
