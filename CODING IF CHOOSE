package selfProgramming;

class IfChoose {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double code=3;
		if(code==1)
			System.out.print("Degree in Computer Science");
		else if(code==2)
			System.out.print("Degree in Multimedia");
		else if(code==3)
			System.out.print("Degree in Software Engineering");

	}

}
